{
    "name": "ntuangiang/php-rest",
    "type": "project",
    "description": "The Php Rest Framework.",
    "keywords": [
        "rest-api",
        "php-rest"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Tuan Giang",
            "email": "ntuangiang@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ntuangiang/php-rest/issues"
    },
    "require": {
        "php": "^7.3",
        "ntuangiang/php-rest-core": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "dev-master": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
